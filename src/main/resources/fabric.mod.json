{
  "schemaVersion": 1,
  "id": "json-entity-animation",
  "version": "${version}",
  "name": "Json Entity Animation",
  "description": "Library mod for creating your entity animations in JSON",
  "authors": [],
  "contact": {
    "repo": "https://github.com/Gaming32/json-entity-animation"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.gaming32.jsonentityanimation.client.JsonEntityAnimationClient"
    ]
  },
  "mixins": [
    "json-entity-animations.mixins.json"
  ],
  "icon": "assets/json-entity-animation/icon.png",
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=1.20.1",
    "fabric-resource-loader-v0": "*"
  },
  "custom": {
    "modmenu": {
      "badges": ["library"]
    }
  }
}
